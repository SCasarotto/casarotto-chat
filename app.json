{
  "expo": {
    "name": "casarotto-chat",
    "slug": "casarotto-chat",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBarColor": "#c17272",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#c17272"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.38thSS.casarottochat"
    },
    "android": {
      "package": "com.thirtyEigthSS.casarottochat"
    }
  }
}
